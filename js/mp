// this is using objects and arrays to do the abouve

var skills =
	["troubleshooting", "running" ];

var bio ={
	"name" : "Mark S Powell",
	"role" : "SR Specialist Support",
	"contacts" : {
		"mobile" :"706-476-6300",
		"email" :"mark_powell@bellsouth.net",
		"github": "PapppyPowell",
		"twitter": "XXXXXXX",
		"location" : "Montcello, GA"
	},
	"biopic" : "image/me.jpg",
	"welcomeMessage": "Have a nice day",
	"skills" : skills,
};

var formattedName = HTMLheaderName.replace ( "%data%" , bio.name);
var formattedRole = HTMLheaderRole.replace ( "%data%" , bio.role);
$("#header").prepend(formattedRole);
$("#header").prepend(formattedName);
$("#header").append(bio.contacts);
$("#header").append(bio.welcomeMessage);
$("#header").append(bio.skills);


